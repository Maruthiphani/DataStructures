class Solution {
    public int[] twoSum(int[] nums, int target) {
        
        int n1= nums.length;
        for(int i=0; i<n1; i++)
        {
            for(int j=i+1; j<n1; j++)
            {
                if(nums[i] + nums[j] == target)
                {
                    return new int[]{i, j};
                }
                
            }
        }
        return null;
    }
}
