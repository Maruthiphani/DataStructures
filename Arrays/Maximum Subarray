class Solution {
    public int maxSubArray(int[] nums) {
 
        int output= nums[0];
        int result=nums[0];
        
        for(int i=1; i<nums.length; i++)
        {
           result = Math.max(nums[i], result + nums[i]);
           output = Math.max(output, result);
        }
        
        return output;
        
}
}
